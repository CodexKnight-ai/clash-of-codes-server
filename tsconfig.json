{
	"compilerOptions": {
		"target": "ES2022",
		"module": "commonjs",
		"outDir": "./build",
		"lib": [
			"ES2022"
		],
		"types": [
			"node"
		],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": false,
		"strict": false,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"noImplicitReturns": false,
		"skipLibCheck": true
	}
}